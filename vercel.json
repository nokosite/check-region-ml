{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/validasi",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ]
}
